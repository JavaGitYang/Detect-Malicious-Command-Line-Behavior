    "from sklearn import svm\n",
    "\n",
    "def oneclasssvm(slideWindowDataset):\n",
    "    clf = svm.OneClassSVM(nu=0.1, kernel=\"rbf\", gamma=0.1)\n",
    "    clf.fit(slideWindowDataset)\n",
    "    y_pred_train = clf.predict(slideWindowDataset)\n",
    "    return y_pred_train\n",
    "    "
